/*alt linkler için*/
#altlinkler{
	margin: 0px auto;
	text-align: center;
	font-size: 12px;
	font-weight: bold;
	height:49px;
	float:left;
	background-image:url(images/navi.jpg);
	background-repeat:no-repeat;
	background-position:top left;
	width:100%;
	padding: 0px 0px 0px 20px;
}

#altlinkler ul,
#altlinkler li{
	padding:0px;
	margin:0px;
	list-style-type:none;
}

#altlinkler ul{
	height:49px;
	line-height:49px; 
	background-position:right;
	float:left;
	padding:0px 1px 0px 0px;
	margin:0px 0px 0px 20px;
}

#altlinkler li{
	height:49px;
	line-height:39px;
	display:inline;
	position:relative; 
	float:left; 
	width:100px; 
	text-align:center;
}

#altlinkler li a{
	height:35px;
	width:100px;
	text-align:center;
	float:left;
	background-position:left;
	background-repeat:no-repeat;
}


#altlinkler li a:link,
#altlinkler li a:visited{
	color:#7F7F7F;
	text-decoration:none;
}

#altlinkler li.active a,
#altlinkler li a:hover{
	background-repeat:no-repeat;
	background-position:left;
}


